+/* Device Control : VDD control */
+int dw1000_power_ctrl(struct dw1000_data *dw1000, int onoff)
+{
+	int ret = 0;
+	struct regulator *regulator_vdd_1p8;
+
+	slog("onoff: %d, state: %d\n", onoff, dw1000->state);
+return 0;
+	switch (onoff) {
+	case DW1000_STATE_POWER_ON:
+		if (dw1000->state == DW1000_STATE_POWER_ON)
+			return 0;
+		regulator_vdd_1p8 = devm_regulator_get(&dw1000->dev,
+							"dw1000_1p8");
+		if (IS_ERR(regulator_vdd_1p8) || regulator_vdd_1p8 == NULL) {
+			slog("Failed to get vdd_1p8 regulator\n");
+			return PTR_ERR(regulator_vdd_1p8);
+		}
+
+		ret = regulator_enable(regulator_vdd_1p8);
+		if (ret) {
+			slog("Failed to enable vdd_1p8, rc: %d\n", ret);
+			return ret;
+		}
+
+		dw1000->state = DW1000_STATE_POWER_ON;
+		break;
+
+	case DW1000_STATE_POWER_OFF:
+		if (dw1000->state == DW1000_STATE_POWER_OFF)
+			return 0;
+
+		ret = regulator_disable(regulator_vdd_1p8);
+		if (ret) {
+			slog("Failed to disable vdd_1p8, rc: %d\n", ret);
+			return ret;
+		}
+		dw1000->state = DW1000_STATE_POWER_OFF;
+		break;
+	}
+
+	return ret;
+}
