+static int dw1000_spi_transfer(struct dw1000_data *dw1000,
+	u8 *tx_buf, u8 *rx_buf, int len) 
+{
+	int ret = 0;
+	struct spi_transfer msg = {
+		.tx_buf	= tx_buf,
+		.rx_buf	= rx_buf,
+		.len	= len,
+	};
+
+	mutex_lock(&dw1000->spilock);
+	ret = spi_sync_transfer(dw1000->spi, &msg, 1);
+	if (ret)
+		slog("SPI transfer failed: %d\n", ret);
+	mutex_unlock(&dw1000->spilock);
+
+	return ret;
+}
+
+static int dw1000_spi_transfer_fullduplex(struct dw1000_data *dw1000,
+			u8 *tx_buf, int txlen, u8 *rx_buf, int rxlen) 
+{
+	int ret = 0;
+	struct spi_transfer msg[] = {
+		[0] = { .tx_buf = tx_buf, .len	= txlen, },
+		[1] = { .rx_buf = rx_buf, .len	= rxlen, },
+	};
+
+	mutex_lock(&dw1000->spilock);
+	ret = spi_sync_transfer(dw1000->spi, &msg, 2);
+	if (ret)
+		slog("SPI sync transfer failed: %d\n", ret);
+	mutex_unlock(&dw1000->spilock);
+
+	return ret;
+}
