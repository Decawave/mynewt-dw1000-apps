+/*
+ * Read call-back function for all DW1000 proc entries
+ * It returns number of bytes successfully read
+ */
+static ssize_t dw1000_proc_read(struct file *file, char __user *ubuf,
+				size_t size, loff_t *offset)
+{
+	/* internal buffer */
+	char kbuf[BUF_SIZE] = { 0 };
+	/* temperary buffer for string operations */
+	char temp[BUF_SIZE] = { 0 };
+
+	/* Get i3l board variant value */
+	int val = get_i3l_variant();
+
+	/* Get private data of specific entry */
+	uint32_t priv_data = *((uint32_t *) file->private_data);
+
+	/* Check the offset, offset must be 0 */
+	if (*offset > 0)
+		return EOF;
+
+	/* Check requested size, it must be equal to BUF_SIZE */
+	if (size >= BUF_SIZE)
+		size = BUF_SIZE;
+	else {
+		err_msg("Size must be %d bytes\n", BUF_SIZE);
+		return 0;
+	}
+
+	/* Fill internal buffer with required information */
+	switch (priv_data) {
+	/* Get MODE_SEL0 pin value */
+	case MODE_SEL0_VAL:
+		val = (val >> MODE_SEL0_VAL) & 0x01;
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	/* Get MODE_SEL1 pin value */
+	case MODE_SEL1_VAL:
+		val = (val >> MODE_SEL1_VAL) & 0x01;
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	/* Get MODE_SEL2 pin value */
+	case MODE_SEL2_VAL:
+		val = (val >> MODE_SEL2_VAL) & 0x01;
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	/* Get MODE_SEL3 pin value */
+	case MODE_SEL3_VAL:
+		val = (val >> MODE_SEL3_VAL) & 0x01;
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	/* Get I3L_REVISION string value */
+	case I3L_REVISION_STR:
+		if (!i3l_types_to_string(kbuf, I3L_REVISION_STR))
+			return 0;
+		break;
+
+	/* Get I3L_DISPLAY_TYPE string value */
+	case I3L_DISPLAY_TYPE_STR:
+		if (!i3l_types_to_string(kbuf, I3L_DISPLAY_TYPE_STR))
+			return 0;
+		break;
+
+	/* Get I3L_VARIANT string value */
+	case I3L_VARIANT_STR:
+		if (!i3l_types_to_string(temp, I3L_DISPLAY_TYPE_STR))
+			return 0;
+		if (!i3l_types_to_string(kbuf, I3L_REVISION_STR))
+			return 0;
+		/* Create variant string */
+		strcpy((kbuf + strlen(kbuf) - 1), "_");
+		strcat(kbuf, temp);
+		break;
+
+	/* Get I3L_REVISION value */
+	case I3L_REVISION_VAL:
+		val = get_i3l_revision();
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	/* Get I3L_DISPLAY_TYPE value */
+	case I3L_DISPLAY_TYPE_VAL:
+		val = get_i3l_display_type();
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	/* Get I3L_VARIANT value */
+	case I3L_VARIANT_VAL:
+		val = get_i3l_variant();
+		sprintf(kbuf, "%d\n", val);
+		break;
+
+	default:
+		err_msg("Invalid read operation type.\n");
+		return 0;
+	}
+
+	/* Find length of kernel buffer including NULL */
+	size = strlen(kbuf) + 1;
+
+	/* Copy prepared string to user buffer */
+	val = copy_to_user(ubuf, kbuf, size);
+	val = size - val;
+	/* Update offset */
+	*offset += val;
+
+	return val;
+}
