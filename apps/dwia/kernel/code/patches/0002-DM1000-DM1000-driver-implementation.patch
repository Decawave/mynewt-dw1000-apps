+#include<stdio.h>
+#include<stdlib.h>
+#include<errno.h>
+#include<fcntl.h>
+#include<string.h>
+#include<unistd.h>
+ 
+#define DW1000_BUF_LEN 256
+#define EN_PATH "/sys/class/input/input0/enable"
+#define SPI_BPW "/sys/class/input/input0/spi_bpw"
+#define SPI_FREQ "/sys/class/input/input0/spi_freq"
+#define SPI_MODE "/sys/class/input/input0/spi_mode"
+#define DPI_TRANSFER "/sys/class/input/input0/spi_transfer"
+#define DW1000_FILE "/dev/dw1000"
+
+static int set_dw1000_enable_state(unsigned short val)
+{
+	int fd;
+	fd = open(EN_PATH, O_RDWR);
+	if (fd < 0) {
+		perror("Failed to open the file...");
+		return errno;
+	}
+
+	if (val == 1)
+		write(fd, "1", 4);
+	else if (val == 0)
+		write(fd, "0", 4);
+	else {
+		printf("Invalid Value\n");
+		return -EINVAL;
+	}
+
+	return 0;
+}
+
+static int get_dw1000_enable_state(void)
+{
+	int fd;
+	char buf[4] = {0};
+	int val = -1;
+
+	fd = open(EN_PATH, O_RDWR);
+	if (fd < 0) {
+		perror("Failed to open the file...");
+		return errno;
+	}
+
+	read(fd, buf, 4);
+	val = atoi(buf);
+	if (val < 0 && val > 1) {
+		printf("Invalid Value\n");
+		return -EINVAL;
+	}
+
+	return val;
+}
+
+//////////////////////////////////////////////////////////////
+
+int print_hex_data(char *rx_buf, int size)
+{
+	int i, j;
+	printf("%s(): size: %d\n", __func__, size);
+#if 0
+	for (i = 0; i < size; i+=16) {
+		printf("%02X | ", i);
+		for (j = 0; j < 16 && (i+j) < size; j++) {
+			printf("%02x ", rx_buf[i+j]);
+		}
+		printf("\n");
+	}
+#else
+	for (i = 0; i < size; i+=16) {
+		int ii;
+		char buf[80] = {0};//[48+4+4+16]
+		sprintf(buf, "%02x| ", i);
+		for (j = 0; j < 16 ; j++) {
+			if (!((i+j) < size)) {
+				sprintf(&buf[4+(j*3)], "   ");
+				sprintf(&buf[4+48+4+j], "%c", ' ');
+				
+			} else {
+				sprintf(&buf[4+(j*3)], "%02X ", rx_buf[i+j]);
+				sprintf(&buf[4+48+4+j], "%c", (rx_buf[i+j] < 126 && rx_buf[i+j] > 21) ? rx_buf[i+j] : '.');
+			}
+
+		}
+		sprintf(&buf[4+48], "   ");
+		buf[4+48+3] = 0x20;
+		printf("%s\n", buf);
+	}
+#endif
+
+	return 0;
+}
+
+static char rx_buf[DW1000_BUF_LEN];
+ 
+int main()
+{
+	int ret;
+	int fd;
+	char tx_buf[DW1000_BUF_LEN];
+
+	fd = open(DW1000_FILE, O_RDWR);
+	if (fd < 0) {
+		perror("Failed to open the file...");
+		return errno;
+	}
+
+	printf("Writeing Start command into TX buffer...\n");
+	strcpy(tx_buf, "INIT");
+	write(fd, tx_buf, DW1000_BUF_LEN);
+
+	printf("Enabling device...\n");
+	ret = set_dw1000_enable_state(1);
+	if (ret < 0)
+		return ret;
+
+	printf("Reading data from RX, for every 3 sec...\n");
+	while(1) {
+		int rd = 0;
+	system("date");
+		rd = read(fd, rx_buf, DW1000_BUF_LEN);
+		if (rd < 0)
+			perror("read:");
+		print_hex_data(rx_buf, rd);
+		sleep(3);
+	}
+
+	return 0;
+}
