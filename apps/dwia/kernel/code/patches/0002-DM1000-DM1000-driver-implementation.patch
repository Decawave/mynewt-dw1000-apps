+static int dw1000_enable(struct dw1000_data *dw1000)
+{
+	int ret = 0;
+
+	ret = dw1000_power_ctrl(dw1000, DW1000_STATE_POWER_ON);
+	if (ret)
+		return ret;
+
+	memset(dw1000->tx_buf, 0x00, sizeof(dw1000->tx_buf));
+	ret = dw1000_spi_transfer(dw1000, dw1000->tx_buf, NULL,
+					sizeof(dw1000->tx_buf));
+	if (ret)
+		return ret;
+
+	memset(dw1000->tx_buf, 0x00, sizeof(dw1000->tx_buf));
+	dw1000->tx_buf[0] = DW1000_CMD_C0;
+	ret = dw1000_spi_transfer(dw1000, dw1000->tx_buf, dw1000->rx_buf,
+					sizeof(dw1000->tx_buf));
+	if (ret)
+		return ret;
+
+	dw1000_irq_set_state(dw1000, DW1000_IRQ_STATE_EN);
+	atomic_set(&dw1000->is_enable, 1);
+
+	slog("dw1000 is enabled\n");
+	return ret;
+}
+
+static void dw1000_disable(struct dw1000_data *dw1000)
+{
+	int ret;
+
+	dw1000_irq_set_state(dw1000, DW1000_IRQ_STATE_DIS);
+	atomic_set(&dw1000->is_enable, 0);
+
+	dw1000_power_ctrl(dw1000, DW1000_STATE_POWER_OFF);
+	if (ret)
+		return ret;
+
+	slog("dw1000 is disabled\n");
+}
