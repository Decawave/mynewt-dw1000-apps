+static int dw1000_spi_probe(struct spi_device *spi)
+{
+	struct dw1000_data *dw1000 = NULL;
+	struct device *dev = &spi->dev; 
+	int ret = 0;
+
+	slog("\n");
+
+	/* Setup SPI bus */
+	spi->bits_per_word = 8;
+	ret = spi_setup(spi);
+	if (ret)
+		return ret;
+
+	if (!dev->of_node)
+		return -ENODEV;
+
+	slog("setup mode: %d, %u bits/w, %u Hz max\n",
+		(int) (spi->mode & (SPI_CPOL | SPI_CPHA)),
+		spi->bits_per_word, spi->max_speed_hz);
+
+	/* Allocate memory for DW1000 data */
+	dw1000 = devm_kzalloc(dev, sizeof(*dw1000), GFP_KERNEL);
+	if (dw1000 == NULL) {
+		slog("Failed to allocate memory\n");
+		return -ENOMEM;
+	}
+
+	/* Init dw1000_data object */
+	dw1000_init_data(dw1000);
+	dw1000->spi = spi;
+	dw1000->dev = dev;
+	spi_set_drvdata(spi, dw1000);
+
+	mutex_init(&dw1000->spilock);
+	mutex_init(&dw1000->activelock);
+
+	ret = dw1000_parse_dt(dw1000, &spi->dev);
+	if (ret < 0)
+		goto err;
+
+	ret = dw1000_irq_setup(dw1000);
+	if (ret)
+		goto err;
+
+	ret = dw1000_power_ctrl(dw1000, DW1000_STATE_POWER_ON);
+	if (ret) 
+		goto err;
+
+	usleep_range(1000, 1100);
+
+	ret = dw1000_power_ctrl(dw1000, DW1000_STATE_POWER_OFF);
+	if (ret)
+		goto err;
+
+	/*Input Device*/
+	dw1000->input_dev = devm_input_allocate_device(dev);
+	if (!dw1000->input_dev) {
+		slog("Error while allocating input device\n");
+		goto err;
+	}
+
+	input_set_drvdata(dw1000->input_dev, dw1000);
+	dw1000->input_dev->name = DW1000_NAME;
+	dw1000->input_dev->phys = "spi/dw1000";
+	input_set_capability(dw1000->input_dev, EV_REL, REL_X);
+	ret = input_register_device(dw1000->input_dev);
+	if (ret < 0) {
+		slog("Failed to register input device\n");
+		goto err;
+	}
+
+#if PROCFS_ENABLE
+	/*
+	 * Create directory structure in '/proc/dw1000/'
+	 */
+	ret = create_table(dw1000, dw1000_proc_entries_table);
+	if (ret < 0)
+		goto err_pde_tbl;
+#else
+	ret = sysfs_create_group(&dw1000->input_dev->dev.kobj, &dw1000_attribute_group);
+	if (ret) {
+		slog("Failed to create sysfs group\n");
+		goto err_sysfs;
+	}
+#endif
+#if 1
+	ret = dw1000_chrdev_create(dw1000);
+	if (ret)
+		goto err_sysfs;
+#endif
+	atomic_set(&dw1000->is_enable, 0);
+
+	slog("Probe successful\n");
+	return 0;
+
+#if PROCFS_ENABLE
+ err_pde_tbl:
+#else
+ err_sysfs:
+#endif
+	input_unregister_device(dw1000->input_dev);
+ err:
+	mutex_destroy(&dw1000->spilock);
+	mutex_destroy(&dw1000->activelock);
+	slog("Probe failed!!!\n");
+	return ret;
+}
+
+static int dw1000_spi_remove(struct spi_device *spi)
+{
+	struct dw1000_data *dw1000 = spi_get_drvdata(spi);
+	slog();
+
+#if PROCFS_ENABLE
+	/* Remove all created table entries from '/proc/dw1000' */
+	remove_table(dw1000_proc_entries_table);
+#else
+	sysfs_remove_group(&dw1000->input_dev->dev.kobj,
+				&dw1000_attribute_group);
+#endif
+	input_unregister_device(dw1000->input_dev);
+	mutex_destroy(&dw1000->spilock);
+	mutex_destroy(&dw1000->activelock);
+#if 1
+	dw1000_chrdev_destroy(dw1000);
+#endif
+
+	return 0;
+}
