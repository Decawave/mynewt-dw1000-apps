+static int dw1000_chrdev_create(struct dw1000_data *dw1000)
+{
+	int ret = 0;
+	struct device *device;
+
+	ret = alloc_chrdev_region(&dw1000->devt, 0, 1, DW1000_NAME);
+	if (ret) {
+		slog("Failed to allocate char dev region\n");
+		return ret;
+	}
+
+	cdev_init(&dw1000->cdev, &dw1000_fops);
+	dw1000->cdev.owner = THIS_MODULE;
+
+	/* Add the device */
+	ret = cdev_add(&dw1000->cdev, dw1000->devt, 1);
+	if (ret < 0) {
+		slog("Cannot register a device,  err: %d", ret);
+		goto err_cdev;
+	}
+
+	dw1000->class = class_create(THIS_MODULE, DW1000_NAME);
+	if (IS_ERR(dw1000->class)) {
+		ret = PTR_ERR(dw1000->class);
+		slog("Unable to create dw1000 class, err: %d\n", ret);
+		goto err_class;
+	}
+
+	dw1000->class->devnode = dw1000_devnode;
+	device = device_create(dw1000->class, dw1000->dev, dw1000->devt, NULL, DW1000_NAME);
+	if (IS_ERR(device)) {
+		ret = PTR_ERR(device);
+		slog("Unable to create dw1000 device, err: %d\n", ret);
+		goto err_device;
+	}
+
+	return 0;
+
+ err_device:
+	class_destroy(dw1000->class);
+ err_class:
+	cdev_del(&dw1000->cdev);
+ err_cdev:
+	unregister_chrdev_region(dw1000->devt, 1);
+
+	return ret;
+}
