+static int dw1000_parse_dt(struct dw1000_data *dw1000, struct device *dev)
+{
+	struct device_node *node = dev->of_node;
+	enum of_gpio_flags flags;
+
+	dw1000->irq_gpio = of_get_named_gpio_flags(node,
+				"dw1000,irq-gpio", 0, &flags);
+	if (!gpio_is_valid(dw1000->irq_gpio)) {
+		slog("Failed to get gpio number\n");
+		return -EINVAL;
+	}
+
+	return 0;
+}
+
+static int dw1000_irq_setup(struct dw1000_data *dw1000)
+{
+	int ret = 0;
+
+	ret = devm_gpio_request(dw1000->dev, dw1000->irq_gpio, "dw1000_int");
+	if (ret) {
+		slog("Failed to request 'dw1000_int'\n");
+		return ret;
+	}
+
+	ret = gpio_direction_input(dw1000->irq_gpio);
+	if (ret) {
+		slog("Failed to set irq_gpio as input\n");
+		return ret;
+	}
+
+	dw1000->irq = gpio_to_irq(dw1000->irq_gpio);
+	if (dw1000->irq < 0) {
+		slog("Invalid IRQ numberi: %d\n", dw1000->irq);
+		return dw1000->irq;
+	}
+
+	ret = devm_request_threaded_irq(dw1000->dev, dw1000->irq, NULL,
+		dw1000_irq_handler, IRQF_TRIGGER_FALLING | IRQF_ONESHOT,
+			DW1000_NAME, dw1000);
+	if (ret < 0) {
+		slog("Failed to request threaded irq: %d\n", ret);
+		return ret;
+	}
+
+	disable_irq(dw1000->irq);
+	return ret;
+}
+
+static void dw1000_irq_set_state(struct dw1000_data *dw1000, int irq_enable)
+{
+	slog("irq_enable: %d, irq_state: %d\n",
+		irq_enable, dw1000->irq_state);
+
+	if (irq_enable) {
+		if (dw1000->irq_state)
+			return;
+		enable_irq(dw1000->irq);
+		dw1000->irq_state = 1;
+	} else {
+		if (dw1000->irq_state == 0)
+			return;
+		disable_irq(dw1000->irq);
+		dw1000->irq_state = 0;
+	}
+}
