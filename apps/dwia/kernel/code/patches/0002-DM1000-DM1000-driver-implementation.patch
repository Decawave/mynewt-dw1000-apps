+/*
+ * create a directory for given table information.
+ * On success returns 0.
+ * On failure return negative value.
+ */
+static int create_directory(struct table_info *table)
+{
+	int entry = 0;
+
+	/* Create a directory according to given table */
+	table->pde = proc_mkdir(table->name, table->parent);
+	if (table->pde == NULL) {
+		slog("Unable to create '%s'\n", table->name);
+		return -EINVAL;
+	}
+
+	/* Update parent information to all child entries */
+	for (entry = 0; table->child[entry].name; entry++)
+		table->child[entry].parent = table->pde;
+
+	return 0;
+}
+
+/*
+ * Create entry for given table information.
+ * On success returns 0.
+ * On failure returns negative value.
+ */
+static int create_entry(struct table_info *table)
+{
+
+	/* Create an entry according to given table */
+	table->pde =
+	    proc_create_data(table->name, table->mode, table->parent,
+			     &dw1000_proc_fops, &table->priv_data);
+	if (table->pde == NULL) {
+		err_msg("Unable to create '%s'\n", table->name);
+		return -EINVAL;
+	}
+
+	return 0;
+}
+
+/*
+ * create_table function creates the directories and files
+ * in the '/proc/dw1000/' according to the given table
+ * information.
+ * Here, Every directory treated as a Table.
+ *
+ * On success returns 0.
+ * On failure returns negative value(< 0).
+ */
+static int create_table(struct dw1000_data *dw1000, struct table_info *table)
+{
+	int tbl_index = 0;
+	int ret = 0;
+
+	/* Create entry for each table index */
+	for (tbl_index = 0; table[tbl_index].name; tbl_index++) {
+		table[tbl_index].priv_data = dw1000;
+		/* Check the table index for directory or file */
+		if (table[tbl_index].child) {
+			/* Create directory in the given path */
+			ret = create_directory(&table[tbl_index]);
+			if (ret < 0)
+				return ret;
+
+			/* Create a child table, by using create_table() call */
+			ret = create_table(table[tbl_index].child);
+			if (ret < 0)
+				return ret;
+
+		} else {
+			/* Create entry in the given path */
+			ret = create_entry(&table[tbl_index]);
+			if (ret < 0)
+				return ret;
+		}
+	}
+
+	return 0;
+}
+
+/* remove_table removes all the created entries in '/proc/dw1000' */
+static void remove_table(struct table_info *table)
+{
+	int tbl_index = 0;
+	for (tbl_index = 0; table[tbl_index].name; tbl_index++)
+		proc_remove(table[tbl_index].pde);
+
+}
